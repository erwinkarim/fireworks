- features.each do |feature|
	.panel.panel-default{ :data => { :'init-features' => 'false' , :licserver => licserver.id, 
		:feature => feature[:name] } }
		.panel-heading{ :role => 'tab' }
			%h4.panel-title
				%a.accordion-toggle{ :data => { :toggle => 'collapse', :parent => '#licserver-' + licserver.id.to_s + '-features-listing' }, :href => '#licserver-' + licserver.id.to_s + '-features-' + feature[:name].sub('/', '-') }
					= feature[:name]
		.panel-collapse.collapse{ :id => 'licserver-' + licserver.id.to_s + '-features-' + feature[:name].sub('/', '-')  }
			.panel-body
				.features-watch
				.features-info
					- if ads_user_signed_in? then
						= watch_icon({ :user => current_ads_user, :model_type => 'FeatureHeader', :model_id => feature[:id], :class => 'btn btn-default', :text => true })
					= link_to fa_icon('info', :text => 'More Info'), licserver_feature_path(licserver.id, feature[:name]), :class => 'btn btn-default' 
				.features-graph
