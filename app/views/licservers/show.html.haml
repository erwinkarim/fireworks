.page-header
	%h1 
		- if ads_user_signed_in? then
			= watch_icon({ :user => current_ads_user, :model_type => 'Licserver', :model_id => @licserver.id })
		= @licserver.port.to_s + '@' + @licserver.server
	%ul.breadcrumb
		%li
			%a{ :href => licservers_path } Licservers
			%span.divider /
		%li
			= @licserver.port.to_s + '@' + @licserver.server
.row
	.span12.info{ :data => { :licserver => @licserver.id } }
