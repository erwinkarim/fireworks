- @licserver.feature_headers.where{ last_seen.gt 1.week.ago }.each do |feature|
  - collapse_id = SecureRandom.hex(6)
  %a.list-group-item{ :href => "##{collapse_id}", :'data-toggle' => 'collapse'} #{ feature.name }
  .collapse.feature-collapse{ :id => collapse_id, :data => { :licserver => @licserver.id, :feature => feature.name }  }
    .list-group-item
      %p
        Graph and info goes here
